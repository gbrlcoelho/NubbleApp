{
  "name": "NubbleApp",
  "version": "6.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start --reset-cache",
    "test": "jest --verbose",
    "coverage": "jest --coverage",
