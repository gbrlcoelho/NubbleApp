    "@shopify/restyle": "^2.4.2",
    "@tanstack/react-query": "^4.35.3",
    "axios": "^1.4.0",
    "date-fns": "^2.30.0",
    "expo": "^50.0.0",
    "expo-image-manipulator": "^11.8.0",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-hook-form": "^7.44.3",
    "react-native": "0.73.6",
